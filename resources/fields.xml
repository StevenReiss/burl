<BURL>
   <FIELDS>
      <FIELD NAME="LCCN" MARC="010a" OPENLIB="lccn" GOOGLE="lccn" LCCN='T' />
      <FIELD NAME="Personal Name" MARC="100a q b c d e" OPENLIB="author_name" GOOGLE="authors"
		ALTNAMES="Primary,Primary Author" FIX="LAST_FIRST" SORT='T' />
      <FIELD NAME="Main Title" MARC="245a b c" OPENLIB="title" GOOGLE="title: subtitle"
		ALTNAMES="Title" SORT='T' />
      <FIELD NAME="Edition" MARC="250a b" />
      <FIELD NAME="Imprint" MARC="260a b c+264a b c" OPENLIB="publisher : publish_year"
		GOOGLE="publisher : publishedData" MULTIPLE="T" ALTNAMES='Publisher' SORT='T' />
      <FIELD NAME="Description" MARC="300a b c e" GOOGLE="description"
		ALTNAMES="Desc" />
      <FIELD NAME="ISBN" ISBN="ORIGINAL" FIX="ISBN"  SORT='T' />
      <FIELD NAME="All ISBNs" ISBN='ALL' MARC="020a" OPENLIB="isbn" FIX="ISBN"
	       GOOGLE="industryIdentifiers.identifier" MULTIPLE="T" ALTNAMES="ISBNs" />
      <FIELD NAME="LC Classification" MARC="050a b" OPENLIB="lcc" ALTNAMES="LCC" FIX="LCC_CODE" SORT='T' />
      <FIELD NAME="Related Names" MARC="700a q b c d e" OPENLIB="author_name" GOOGLE="authors" MULTIPLE="T"
		ALTNAMES='Author,Authors' SORT='T' />
      <FIELD NAME="Contents" MARC="505a" MULTIPLE='T' />
      <FIELD NAME="Subjects" MARC="630a -- x -- v+650a -- x -- v+651a -- e -- g -- 4 -- v -- x -- y -- z"
		OPENLIB="subject" GOOGLE="categories" MULTIPLE="T" ALTNAMES='Subject,subj' SORT='T' VIEWMULT='T' />
      <FIELD NAME="Notes" MARC="504a" MULTIPLE="T" />
      <FIELD NAME="Series" MARC="440a n v+490a x*a v x" MULTIPLE="T" />
      <FIELD NAME="Version" MARC="251a" />
      <FIELD NAME="Dewey Class No." MARC="082a--2" OPENLIB="ddc" FIELDNAME="dewey"
		ALTNAMES='Dewey,ddn' SORT='T' />
      <FIELD NAME="OCLC" MARC="035a" OPENLIB="oclc" GOOGLE="oclc" />
      <FIELD NAME="Copy" DEFAULT="1" />
      <FIELD NAME="Shelf" SORT='T' />
      <FIELD NAME="Status" />
      <FIELD NAME="Labeled" LABELED="T" FIX="YES_NO" DEFAULT="no" />
   </FIELDS>
   <CSV QUOTE='"' SEPARATOR="," />
   <MULTIPLE TEXT=" | " />
   <LABEL CODE='LC Classification' AUTHOR='Personal Name,Related Names' />
</BURL>
