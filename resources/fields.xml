<BURL>
   <FIELDS>
      <FIELD NAME="LCCN" MARC="010a" OPENLIB="lccn" GOOGLE="lccn" LCCN='T' />
      <FIELD NAME="Personal Name" MARC="100 aqbcde" OPENLIB="author_name" GOOGLE="authors"
		ALTNAMES="Primary,Primary Author,personalname" FIX="LAST_FIRST" SORT='T' />
      <FIELD NAME="Main Title" MARC="245 abc" OPENLIB="title" GOOGLE="title: subtitle"
		ALTNAMES="Title" SORT='T' SORTTYPE='TITLE' />
      <FIELD NAME="Edition" MARC="250a b" />
      <FIELD NAME="Imprint" MARC="260 abc+264 abc" OPENLIB="publisher : publish_year"
		GOOGLE="publisher : publishedDate" MULTIPLE="T" ALTNAMES='Publisher' SORT='T' />
      <FIELD NAME="Date" MARC="260c|264c" OPENLIB="publish_year" GOOGLE="publishedDate"
		SORT='T' FIX='DATE' />
      <FIELD NAME="Description" MARC="300 abce" GOOGLE="description"
		ALTNAMES="Desc" />
      <FIELD NAME="ISBN" ISBN="ORIGINAL" FIX="ISBN"  SORT='T' />
      <FIELD NAME="All ISBNs" ISBN='ALL' MARC="020a" OPENLIB="isbn" FIX="ISBN"
	       GOOGLE="industryIdentifiers.identifier" MULTIPLE="T" ALTNAMES="ISBNs,allisbns" />
      <FIELD NAME="LC Classification" MARC="050 ab" OPENLIB="lcc" ALTNAMES="LCC,lcclassifaction"
		FIX="LCC_CODE" SORT='F' UPDATE="lccsort" />
      <FIELD NAME="Volume" MARC="490v|440v" />
      <FIELD NAME="Related Names" MARC="700 aqbcde" OPENLIB="author_name" GOOGLE="authors" MULTIPLE="T"
		ALTNAMES='Author,Authors,relatednames' SORT='T' />
      <FIELD NAME="Contents" MARC="505a" MULTIPLE='T' />
      <FIELD NAME="Subjects" MARC="630 -- axv+650 -- axv+651 -- aeg4vxyz"
		OPENLIB="subject" GOOGLE="categories" MULTIPLE="T" ALTNAMES='Subject,subj'
		SORT='T' VIEWMULT='T' />
      <FIELD NAME="Notes" MARC="500a+504a" MULTIPLE="T" />
      <FIELD NAME="Series" MARC="440 anv+490 avx+830 anv" MULTIPLE="T" />
      <FIELD NAME="Version" MARC="251a" />
      <FIELD NAME="Dewey Class No." MARC="082--a2" OPENLIB="ddc" FIELDNAME="dewey"
		ALTNAMES='Dewey,ddn,deweyclassno' SORT='T' />
      <FIELD NAME="OCLC" MARC="035a" OPENLIB="oclc" GOOGLE="oclc" />
      <FIELD NAME="Copy" DEFAULT="1" FIX="DEFAULT" />
      <FIELD NAME="Shelf" SORT='T' GROUPEDIT='T' />
      <FIELD NAME="Status" />
      <FIELD NAME="Print Labels" LABELED="T" FIX="YES_NO" DEFAULT="no"
		ALTNAMES='printlabels' DISPLAY="YES_NO" />
      <FIELD NAME="Verified" FIX="YES_NO" DEFAULT="no" DISPLAY="YES_NO" />
      <FIELD NAME="LCC Shelf Order" FIX="LCC_SORT" HIDDEN="TRUE" SORT="T" FIELDNAME="lccsort" />
   </FIELDS>
   <CSV QUOTE='"' SEPARATOR="," />
   <MULTIPLE TEXT=" | " />
   <LABEL CODE='LC Classification' AUTHOR='Personal Name,Related Names' YEAR='Date'
		VOLUME='Volume' COPY='Copy' />
</BURL>
